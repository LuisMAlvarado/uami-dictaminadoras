{% extends 'base.html.twig' %}

{% block body %}
    <h1>Dictaman</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ dictaman.id }}</td>
            </tr>
            <tr>
                <th>Numdictamen</th>
                <td>{{ dictaman.numDictamen }}</td>
            </tr>
            <tr>
                <th>Fechadictmen</th>
                <td>{% if dictaman.fechaDictmen %}{{ dictaman.fechaDictmen|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Nivelasignado</th>
                <td>{{ dictaman.nivelAsignado }}</td>
            </tr>
            <tr>
                <th>Modalidades</th>
                <td>{{ dictaman.modalidades }}</td>
            </tr>
            <tr>
                <th>Argumento</th>
                <td>{{ dictaman.argumento }}</td>
            </tr>
            <tr>
                <th>Asesores</th>
                <td>{{ dictaman.asesores }}</td>
            </tr>
            <tr>
                <th>Pdfdictamen</th>
                <td>{{ dictaman.pdfDictamen }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('dictamen_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('dictamen_edit', { 'id': dictaman.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
